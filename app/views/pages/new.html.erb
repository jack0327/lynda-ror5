<%=link_to("<<Back to List", pages_path, :class=>'back-link')%>
  <div class="page new">
    <h2>Create Page</h2>
    <%= form_for(@page) do |f|%>
    <table summary = "Pages from fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name)%></td>
      </tr>
      <tr>
        <th>Position</th>
        <td><%=f.text_field(:position)%></td>
      </tr>
      <tr>
        <th>description</th>
        <td><%= f.text_field(:description)%></td>
      </tr>
      <tr>
        <!-- <th>hidden</th> -->
        <td><%= f.hidden_field(:subject_id)%></td>
      </tr>
    </table>

    <div class = "form-buttons">
      <%= f.submit("Create Page")%>
    </div>
    <%end%>
      </div>
